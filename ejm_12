#include <iostream>
#include <queue>
using namespace std;

queue <int> cola;

int main()
{
	cola.push(10);
	cola.push(8);
	cola.push(50);
	
	cout<<"Tamano "<<cola.size()<<endl;
	cout<<"Estado de la cola "<<cola.empty()<<endl;
	cout<<"Primer elemento "<<cola.front()<<endl;
	cout<<"Ultimo elemento "<<cola.back()<<endl;
	cola.pop();
	cout<<"Primer elemento "<<cola.front()<<endl;
	cout<<"Ultimo elemento "<<cola.back()<<endl;
	
}
